window.Project ||= {}
Project.show = () ->
	$ ->
	  new Highcharts.Chart(
	  	chart:
	    	renderTo: "projectchart"
	    title:
	    	text: gon.project.name
	    xAxis:
	    	type: 'datetime'
	    	title: 
	    		text:gon.project.x_name
	    yAxis: 
	    	title: 
	    		text: gon.project.y_name 
	    series: [data: $.map(gon.project.data_values, (item, index) -> item.value) ]
	  )